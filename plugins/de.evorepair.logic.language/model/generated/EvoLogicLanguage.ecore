<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="evoLogicLanguage" nsURI="http://tu-braunschweig.de/isf/darwinspl/evorepair/logic/firstorder"
    nsPrefix="evoLogicLanguage">
  <eClassifiers xsi:type="ecore:EClass" name="CommandDocument">
    <eStructuralFeatures xsi:type="ecore:EReference" name="imports" upperBound="-1"
        eType="#//Import" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="formula" eType="#//EvoFormula"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Import">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="importURI" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoFormula">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variables" upperBound="-1"
        eType="#//EvoVariable" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="term" eType="#//EvoAbstractTerm"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoVariable"/>
  <eClassifiers xsi:type="ecore:EClass" name="EvoVariableTerm" eSuperTypes="#//EvoAbstractTerm">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variable" eType="#//EvoVariable"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoArbitratyFeatureVariable" eSuperTypes="#//EvoVariable">
    <eStructuralFeatures xsi:type="ecore:EReference" name="feature" eType="ecore:EClass platform:/resource/eu.hyvar.feature/model/Feature.ecore#//HyFeature"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoAbstractTerm"/>
  <eClassifiers xsi:type="ecore:EClass" name="EvoOr" eSuperTypes="#//EvoAbstractTerm">
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftElement" eType="#//EvoAbstractTerm"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rightElement" eType="#//EvoAbstractTerm"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoXOr" eSuperTypes="#//EvoAbstractTerm">
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftElement" eType="#//EvoAbstractTerm"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rightElement" eType="#//EvoAbstractTerm"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvoAnd" eSuperTypes="#//EvoAbstractTerm">
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftElement" eType="#//EvoAbstractTerm"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rightElement" eType="#//EvoAbstractTerm"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
